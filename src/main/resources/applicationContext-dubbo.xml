<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans.xsd    
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<dubbo:registry protocol="zookeeper" address="zookeeper://127.0.0.1:2181?client=zkclient" />
	
	<dubbo:application name="jzcConsumption"/>
	
	<dubbo:reference id="productService" interface="com.domain.service.ProductService" check="false" version="1.0"/>

</beans>